{
  "name": "@britt/llpm",
  "version": "1.8.0",
  "description": "Large Language Model Product Manager - AI-powered CLI for software development",
  "keywords": [
    "llm",
    "ai",
    "cli",
    "product-manager",
    "development",
    "tools",
    "project-management",
    "github",
    "terminal",
    "chat",
    "openai",
    "anthropic"
  ],
  "author": "britt",
  "license": "MIT",
  "homepage": "https://github.com/britt/llpm#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/britt/llpm.git"
  },
  "bugs": {
    "url": "https://github.com/britt/llpm/issues"
  },
  "module": "index.ts",
  "type": "module",
  "private": false,
  "files": [
    "bin/llpm.cjs",
    "dist/llpm.js",
    "skills/",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18"
  },
  "bin": {
    "llpm": "./bin/llpm.cjs"
  },
  "scripts": {
    "build": "bun build index.ts --target=node --outfile dist/llpm.js --banner 'var parcelRequire;'",
    "prepublishOnly": "bun run build",
    "start": "bun run index.ts",
    "start:verbose": "bun run index.ts --verbose",
    "dev": "bun run index.ts",
    "dev:verbose": "bun run index.ts --verbose",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:coverage:watch": "vitest --coverage --watch",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "lint:all": "bun run lint && bun run format:check && bun run typecheck",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "typecheck:strict": "tsc --noEmit --project tsconfig.strict.json",
    "find:unused": "ts-prune --error --ignore 'index.ts|.test.ts|.test.tsx'",
    "find:unused:verbose": "ts-prune",
    "setup": "bun run index.ts setup"
  },
  "devDependencies": {
    "@ai-sdk/anthropic": "^2.0.5",
    "@ai-sdk/google-vertex": "^3.0.10",
    "@ai-sdk/groq": "^2.0.11",
    "@ai-sdk/openai": "^2.0.16",
    "@arcadeai/arcadejs": "^1.9.1",
    "@octokit/rest": "^22.0.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.65.0",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.206.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.207.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.206.0",
    "@opentelemetry/instrumentation-fs": "^0.26.0",
    "@opentelemetry/instrumentation-http": "^0.206.0",
    "@opentelemetry/resources": "^2.1.0",
    "@opentelemetry/sdk-metrics": "^2.1.0",
    "@opentelemetry/sdk-node": "^0.206.0",
    "@opentelemetry/semantic-conventions": "^1.37.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@types/bun": "latest",
    "@types/js-yaml": "^4.0.9",
    "@types/react": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "3.2.4",
    "ai": "^5.0.8",
    "ansi-escapes": "^7.1.1",
    "axios": "^1.12.2",
    "cli-html": "^4.4.0",
    "cli-markdown": "^3.5.1",
    "cli-spinners": "^3.3.0",
    "clipboardy": "^4.0.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "fullscreen-ink": "^0.1.0",
    "gray-matter": "^4.0.3",
    "happy-dom": "^18.0.1",
    "ignore": "^7.0.5",
    "ink": "^6.1.0",
    "ink-link": "^4.1.0",
    "ink-select-input": "^6.2.0",
    "ink-spinner": "^5.0.0",
    "ink-syntax-highlight": "^2.0.2",
    "ink-table": "^3.1.0",
    "ink-task-list": "^2.0.0",
    "ink-testing-library": "^4.0.0",
    "ink-text-input": "^6.0.0",
    "ink-use-stdout-dimensions": "^1.0.5",
    "js-yaml": "^4.1.0",
    "jsdom": "^26.1.0",
    "memfs": "^4.51.1",
    "openai": "^5.12.2",
    "parse5": "^8.0.0",
    "prettier": "^3.6.2",
    "react": "^19.1.1",
    "ts-prune": "^0.10.3",
    "typescript": "^5",
    "vite": "^7.1.1",
    "vitest": "^3.2.4",
    "zod": "^4.0.17"
  }
}
