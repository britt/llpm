{
  "name": "llpm",
  "version": "0.2.0",
  "description": "Large Language Model Product Manager - AI-powered CLI for software development",
  "keywords": ["llm", "ai", "cli", "product-manager", "development", "tools"],
  "author": "britt",
  "license": "MIT",
  "homepage": "https://github.com/britt/llpm#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/britt/llpm.git"
  },
  "bugs": {
    "url": "https://github.com/britt/llpm/issues"
  },
  "module": "index.ts",
  "type": "module",
  "private": false,
  "bin": {
    "llpm": "./bin/llpm.cjs"
  },
  "scripts": {
    "postinstall": "node scripts/postinstall.cjs",
    "start": "bun run index.ts",
    "start:verbose": "bun run index.ts --verbose",
    "dev": "bun run index.ts",
    "dev:verbose": "bun run index.ts --verbose",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@types/bun": "latest",
    "@types/react": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "3.2.4",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "happy-dom": "^18.0.1",
    "ink-testing-library": "^4.0.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "vite": "^7.1.1",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.5",
    "@ai-sdk/google-vertex": "^3.0.10",
    "@ai-sdk/groq": "^2.0.11",
    "@ai-sdk/openai": "^2.0.16",
    "@octokit/rest": "^22.0.0",
    "ai": "^5.0.8",
    "ink": "^6.1.0",
    "ink-link": "^4.1.0",
    "ink-select-input": "^6.2.0",
    "ink-spinner": "^5.0.0",
    "ink-syntax-highlight": "^2.0.2",
    "ink-table": "^3.1.0",
    "ink-text-input": "^6.0.0",
    "ink-use-stdout-dimensions": "^1.0.5",
    "openai": "^5.12.2",
    "react": "^19.1.1",
    "zod": "^4.0.17"
  }
}
